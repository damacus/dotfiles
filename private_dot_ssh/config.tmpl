Include ~/.ssh/config.hosts

Host github.com
  Hostname ssh.github.com
  Port 443
  User git

Host *
  IdentityAgent ~/.1password/agent.sock
  ServerAliveInterval 60
  {{- if eq .chezmoi.os "darwin" }}
  UseKeychain no
  {{- end }}

ForwardX11 no
ForwardX11Trusted no
ForwardAgent yes
HashKnownHosts no
GSSAPIAuthentication no
